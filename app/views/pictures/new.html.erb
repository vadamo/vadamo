<h1>New Picture</h1>

Name: <%= @picture.name %>

<%= error_messages_for :picture %>

<% form_for(:picture, :url => pictures_path, 
                      :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="artwork">Upload a picture of the  artwork:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Create' %>
  </p>
<% end %>
