<%= error_messages_for :artwork %>

<% form_for(:artwork, :url => artworks_path, 
                      :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="artwork">Upload some artwork:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Create' %>
  </p>
<% end -%>
